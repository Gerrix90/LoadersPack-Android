<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleView">
        <attr name="circleRadius" format="dimension" />
        <attr name="circleColor" format="color" />
        <attr name="circleDrawOnlystroke" format="boolean" />
        <attr name="circleStrokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ArcView">
        <attr name="arcRadius" format="dimension" />
        <attr name="arcWidth" format="dimension" />
        <attr name="startAngle" format="float" />
        <attr name="sweepAngle" format="float" />
        <attr name="arcColor" format="color" />
        <attr name="drawOnlyStroke" format="boolean" />
    </declare-styleable>

    <declare-styleable name="FidgetView">
        <attr name="fidgetRadius" format="dimension" />
        <attr name="fidgetBodyColor" format="color" />
        <attr name="fidgetSideCirclesColor" format="color" />
    </declare-styleable>

    <declare-styleable name="NeedleView">
        <attr name="needleLength" format="dimension" />
        <attr name="needleWidth" format="dimension" />
        <attr name="needleJointRadius" format="dimension" />
        <attr name="needleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="MagnifyingGlassView">
        <attr name="glassRadius" format="dimension" />
        <attr name="glassBorderWidth" format="dimension" />
        <attr name="glassHandleLength" format="dimension" />

        <attr name="glassColor" format="color" />
    </declare-styleable>


    <declare-styleable name="CircularSticksBaseView">
        <attr name="sticksbase_noOfSticks" format="integer" />
        <attr name="sticksbase_outerCircleRadius" format="dimension" />
        <attr name="sticksbase_innerCircleRadius" format="dimension" />
        <attr name="sticksbase_stickColor" format="color" />
        <attr name="sticksbase_viewBackgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ClockLoader">
        <attr name="clock_outerCircleBorderWidth" format="dimension" />
        <attr name="clock_bigCircleRadius" format="dimension" />
        <attr name="clock_innerCircleRadius" format="dimension" />
        <attr name="clock_hourHandLength" format="dimension" />
        <attr name="clock_minuteHandLength" format="dimension" />

        <attr name="clock_outerCircleBorderColor" format="color" />
        <attr name="clock_bigCircleColor" format="color" />
        <attr name="clock_innerCircleColor" format="color" />
        <attr name="clock_hourHandColor" format="color" />
        <attr name="clock_minuteHandColor" format="color" />

        <attr name="clock_animSpeedMultiplier" format="float" />
    </declare-styleable>

    <declare-styleable name="RippleLoader">
        <attr name="ripple_circleInitialRadius" format="dimension" />
        <attr name="ripple_circleColor" format="color" />
        <attr name="ripple_fromAlpha" format="float" />
        <attr name="ripple_toAplha" format="float" />
        <attr name="ripple_animDuration" format="integer" />
        <attr name="ripple_interpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="MultipleRippleLoader">
        <attr name="multipleripple_circleInitialRadius" format="dimension" />
        <attr name="multipleripple_circleColor" format="color" />

        <attr name="multipleripple_noOfRipples" format="integer" />

        <attr name="multipleripple_fromAlpha" format="float" />
        <attr name="multipleripple_toAplha" format="float" />
        <attr name="multipleripple_animDuration" format="integer" />
        <attr name="multipleripple_interpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="RotatingCircularSticksLoader">
        <attr name="rotatingsticks_noOfSticks" format="integer" />
        <attr name="rotatingsticks_outerCircleRadius" format="dimension" />
        <attr name="rotatingsticks_innerCircleRadius" format="dimension" />
        <attr name="rotatingsticks_stickColor" format="color" />
        <attr name="rotatingsticks_viewBackgroundColor" format="color" />
        <attr name="rotatingsticks_animDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="CircularSticksLoader">
        <attr name="circularsticks_noOfSticks" format="integer" />
        <attr name="circularsticks_outerCircleRadius" format="dimension" />
        <attr name="circularsticks_innerCircleRadius" format="dimension" />

        <attr name="circularsticks_stickColor" format="color" />
        <attr name="circularsticks_selectedStickColor" format="color" />
        <attr name="circularsticks_viewBackgroundColor" format="color" />


        <attr name="circularsticks_showRunningShadow" format="boolean" />
        <attr name="circularsticks_firstShadowColor" format="color" />
        <attr name="circularsticks_secondShadowColor" format="color" />

        <attr name="circularsticks_animDuration" format="integer" />
    </declare-styleable>


    <declare-styleable name="CurvesLoader">
        <attr name="curves_noOfCurves" format="integer" />
        <attr name="curves_outermostCurveRadius" format="dimension" />
        <attr name="curves_curveWidth" format="dimension" />
        <attr name="curves_distanceBetweenCurves" format="dimension" />
        <attr name="curves_curveSweepAngle" format="float" />
        <attr name="curves_curveColor" format="color" />
        <attr name="curves_animDuration" format="integer" />
        <attr name="curves_interpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="RingAndCircleLoader">
        <attr name="ringandcircle_circleRadius" format="dimension" />

        <attr name="ringandcircle_ringRadius" format="dimension" />
        <attr name="ringandcircle_ringWidth" format="dimension" />

        <attr name="ringandcircle_circleColor" format="color" />
        <attr name="ringandcircle_ringColor" format="color" />
        <attr name="ringandcircle_animDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="ArcProgressLoader">
        <attr name="arcprogress_arcRadius" format="dimension" />
        <attr name="arcprogress_arcWidth" format="dimension" />

        <attr name="arcprogress_incrementalAngle" format="float" />
        <attr name="arcprogress_maxArcAngle" format="float" />

        <attr name="arcprogress_arcColorsArray" format="reference" />
    </declare-styleable>

    <declare-styleable name="FidgetLoader">
        <attr name="fidget_fidgetRadius" format="dimension" />
        <attr name="fidget_bodyColor" format="color" />
        <attr name="fidget_sideCirclesColor" format="color" />

        <attr name="fidget_noOfRotation" format="integer" />
        <attr name="fidget_animDuration" format="integer" />
        <attr name="fidget_interpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="WifiLoader">
        <attr name="wifi_centerCircleRadius" format="dimension" />
        <attr name="wifi_wifiColor" format="color" />
        <attr name="wifi_incrementalAngle" format="float" />
    </declare-styleable>

    <declare-styleable name="PulseLoader">
        <attr name="pulse_LineThickness" format="dimension" />
        <attr name="pulse_sideLength" format="dimension" />

        <attr name="pulse_normalIncrementalValue" format="float" />
        <attr name="pulse_pulseIncrementalValue" format="float" />

        <attr name="pulse_pulseColor" format="color" />
    </declare-styleable>

    <declare-styleable name="GaugeLoader">
        <attr name="gauge_rangeIndicatorRadius" format="dimension" />
        <attr name="gauge_rangeIndicatorWidth" format="dimension" />
        <attr name="gauge_needleWidth" format="dimension" />
        <attr name="gauge_needleJointRadius" format="dimension" />

        <attr name="gauge_lowerRangeColor" format="color" />
        <attr name="gauge_higherRangeColor" format="color" />
        <attr name="gauge_needleColor" format="color" />

        <attr name="gauge_defaultStartLoading" format="boolean" />
    </declare-styleable>

</resources>